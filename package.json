{
  "name": "apollos-church-api",
  "version": "1.0.0",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "setup": "./scripts/init.sh",
    "start": "yarn start:prod",
    "start:dev": "nodemon ./src/index.js --exec babel-node --delay 2 -e js,yaml,json,yml",
    "start:prod": "NODE_ENV=production node ./lib/index.js",
    "postinstall": "yarn build",
    "build": "NODE_ENV=production rm -rf ./lib && mkdir -p lib && babel ./src -d ./lib",
    "test": "echo \"No tests in apollos-church-api, all tested code is located in vendored apollos modules\"",
    "lint": "eslint ./src --ext .js",
    "translate": "node scripts/translate.js",
    "schema:publish": "node ./scripts/print-schema.js | rover graph publish \"$APOLLO_GRAPH_REF\" --schema -"
  },
  "engines": {
    "node": "24"
  },
  "dependencies": {
    "@apollo/client": "3.3.20",
    "@apollo/rover": "^0.37.0",
    "@babel/cli": "^7.4.5",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/preset-env": "7.4.2",
    "@bugsnag/js": "7.2.1",
    "@graphql-tools/schema": "^10.0.0",
    "algoliasearch": "^3.34.0",
    "analytics-node": "^3.4.0-beta.1",
    "apollo-datasource": "^0.2.0",
    "apollo-datasource-rest": "^0.6.4",
    "apollo-server": "^2.25.4",
    "apollo-server-caching": "^0.4.0",
    "apollo-server-env": "^2.3.0",
    "apollo-server-express": "^2.25.4",
    "awesome-phonenumber": "^2.4.2",
    "bull": "^3.11.0",
    "bull-board": "^0.1.1",
    "cloudinary": "^2.7.0",
    "color": "3.0.0",
    "cors": "^2.8.5",
    "datauri": "^1.1.0",
    "dotenv": "6.0.0",
    "express": "^4.20.0",
    "express-basic-auth": "^1.2.0",
    "form-data": "^4.0.4",
    "graphql": "14.6.0",
    "graphql-parse-resolve-info": "^4.4.5",
    "graphql-upload": "^12.0.0",
    "hyphenation.en-us": "^0.2.1",
    "hypher": "^0.2.5",
    "ioredis": "4.14.1",
    "js-yaml": "^3.14.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.35",
    "natural": "^0.6.2",
    "passkit-generator": "1.5.9",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "qrcode": "^1.3.3",
    "randomcolor": "0.5.3",
    "sanitize-html": "^2.12.1",
    "sequelize": "^6.29.0",
    "twilio": "^3.28.0",
    "umzug": "^3.0.0-beta.15",
    "universal-analytics": "^0.4.20",
    "with-query": "^1.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.2",
    "eslint-plugin-prettier": "^2.6.0",
    "jest": "^25.1.0",
    "nodemon": "1.18.7",
    "prettier": "1.12.1"
  }
}
